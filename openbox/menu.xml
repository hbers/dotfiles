				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Tint2 Configuration Manager">
			<action name="Execute">
				<execute>tint2conf</execute>
			</action>
		</item>
		<separator/>
