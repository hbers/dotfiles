[begin] (FluxBox)
	[submenu] (Editors)
		[exec] (Vim) {urxvt -e vim}
	[end]
	[submenu] (Games)
			[exec] (PPSSPP) {ppsspp}
	[end]
	[submenu] (Graphics)
		[exec] (Feh) {feh}
		[exec] (GNU Image Manipulation Program) {gimp-2.8}
		[exec] (LibreOffice Draw) {libreoffice --draw}
		[exec] (MComix) {mcomix}
		[exec] (MuPDF) {mupdf}
	[end]
	[submenu] (Multimedia)
		[exec] (ALSA mixer) {rxvt -e alsamixer}
		[exec] (mpv Media Player) {mpv --profile=pseudo-gui --}
	[end]
	[submenu] (Network)
		[exec] (Pale Moon Web Browser) {palemoon}
		[exec] (Telegram Desktop) {telegram}
		[exec] (Thunderbird) {thunderbird}
		[exec] (W3m) {rxvt -e w3m}
	[end]
	[submenu] (Office)
		[exec] (LibreOffice Calc) {libreoffice --calc}
		[exec] (LibreOffice Impress) {libreoffice}
		[exec] (LibreOffice Impress) {libreoffice --impress}
		[exec] (LibreOffice Math) {libreoffice --math}
		[exec] (LibreOffice Writer) {libreoffice --writer}
	[end]
	[submenu] (Other)
		[exec] (ncmpcpp) {urxvt -e ncmpcpp}
		[exec] (Redshift) {~/.config/redshifttoggle.sh}
		[exec] (Tint2 panel) {tint2}
		[exec] (Trackma) {trackma-gtk}
	[end]
	[submenu] (Terminal)
		[exec] (urxvt) {urxvt}
		[exec] (urxvt [client]) {urxvtc.sh}
	[end]
	[submenu] (System)
		[submenu] (Settings)
			[submenu] (GNOME)
				[exec] (Adobe Flash Player) {flash-player-properties}
			[end]
			[exec] (NVIDIA X Server Settings) {nvidia-settings}
			[exec] (Tint2 panel settings) {tint2conf}
			[exec] (Xfontsel) {xfontsel}
		[end]
		[exec] (GParted) {gparted_polkit}
		[exec] (Grub Customizer) {grub-customizer}
		[exec] (Htop) {rxvt -e htop}
	[end]
	[submenu] (Utilities)
		[exec] (Archive Manager) {file-roller}
		[exec] (SpaceFM File Search) {spacefm}
		[exec] (Xev) {rxvt -e xev}
	[end]
	[separator]
	[submenu] (FluxBox)
		[workspaces] (Workspaces)
		[submenu] (Styles)
			[stylesdir] (/usr/share/fluxbox/styles)
			[stylesdir] (~/.fluxbox/styles)
		[end]
		[config] (Configure)
		[reconfig] (Reconfig)
		[restart] (Restart)
		[separator]
		[exit] (Exit)
	[end]
[end]
